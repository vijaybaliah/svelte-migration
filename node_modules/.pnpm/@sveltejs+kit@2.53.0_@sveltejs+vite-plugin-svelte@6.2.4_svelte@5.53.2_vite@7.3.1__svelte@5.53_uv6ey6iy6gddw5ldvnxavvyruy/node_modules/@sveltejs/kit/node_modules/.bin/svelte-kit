#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules/@sveltejs/kit/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules/@sveltejs/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules/@sveltejs/kit/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules/@sveltejs/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/@sveltejs+kit@2.53.0_@sveltejs+vite-plugin-svelte@6.2.4_svelte@5.53.2_vite@7.3.1__svelte@5.53_uv6ey6iy6gddw5ldvnxavvyruy/node_modules:/Users/kvb/svelte-migration/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
